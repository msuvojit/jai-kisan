<h1>{{title}}</h1>
<table id="users-table" class="display" width="100%">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Company Name</th>
            <th>Job Title.</th>
            <th>University</th>
            <th>Skill</th>
        </tr>
    </thead>
</table>

<script>
    var users = {{{users}}};

$(document).ready(function() {
$('#users-table').DataTable( {
data: users,
columns: [
    { "data": "id" },
    { "data": "firstName" },
    { "data": "lastName" },
    { "data": "email" },
    { "data": "gender" },
    { "data": "companyName" },
    { "data": "jobTitle" },
    { "data": "university" },
    { "data": "skill" }
]
} );
} );

</script>